<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xsi:schemaLocation="
  http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd
  ">

  <!-- Create a GoogleStockMarket instance -->
  <bean id="stockMarket" class="org.pentaho.karaf.examples.stock.google.GoogleStockMarket" scope="singleton"/>

  <!-- Register instance with OSGI Service Registry so other bundles can find ie -->
  <service ref="stockMarket" auto-export="interfaces"/>

</blueprint>